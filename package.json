{
  "private": true,
  "name": "interv-timer",
  "description": "My application description",
  "version": "0.5.1",
  "author": "Tsubasa Namiki <namikingsoft@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/namikingsoft/interv-timer.git"
  },
  "main": "src/main/main.ts",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "build": "electron-builder",
    "build:dir": "npm run build -- --dir -c.compression=store -c.mac.identity=null",
    "release": "npm run build -- --publish always",
    "test": "jest",
    "e2e": "playwright test",
    "fmt": "prettier --write .",
    "fmtc": "prettier --check .",
    "type": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "upgrade-interactive": "npx npm-check --update",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "net.namiking.interv-timer",
    "productName": "IntervTimer",
    "copyright": "Copyright Â© 2020 Tsubasa Namiki",
    "afterSign": "./bin/after-sign-mac.js",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    }
  },
  "dependencies": {
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-serve": "^1.1.0",
    "electron-store": "6.0.1",
    "electron-updater": "4.3.5",
    "source-map-support": "0.5.19"
  },
  "devDependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@playwright/test": "1.29.1",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "4.0.0",
    "@types/jest": "26.0.20",
    "@types/papaparse": "^5.3.7",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "3.10.0",
    "@typescript-eslint/parser": "3.10.1",
    "classnames": "^2.3.2",
    "css-loader": "5.0.1",
    "electron": "13.6.1",
    "electron-builder": "22.9.1",
    "electron-notarize": "^1.2.2",
    "electron-webpack": "2.8.2",
    "electron-webpack-ts": "4.0.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-standard": "5.0.0",
    "fast-deep-equal": "3.1.3",
    "i18next": "19.8.4",
    "jest": "26.6.3",
    "papaparse": "^5.3.2",
    "playwright": "1.29.1",
    "prettier": "^2.8.1",
    "react": "16.14.0",
    "react-dom": "16.13.1",
    "react-i18next": "11.7.4",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.13.1",
    "redux": "^4.2.0",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.3",
    "ts-jest": "26.4.4",
    "typeface-roboto": "1.1.13",
    "typescript": "3.9.9",
    "webpack": "4.44.2"
  }
