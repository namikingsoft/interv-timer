{
  "private": true,
  "name": "interv-timer",
  "description": "My application description",
  "version": "0.5.1",
  "author": "Tsubasa Namiki <namikingsoft@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/namikingsoft/interv-timer.git"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "vite",
    "compile": "vite build",
    "build": "electron-builder",
    "build:dir": "npm run build -- --dir -c.compression=store -c.mac.identity=null",
    "release": "npm run build -- --publish always",
    "test": "jest",
    "e2e": "playwright test",
    "fmt": "prettier --write .",
    "fmtc": "prettier --check .",
    "type": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "upgrade-interactive": "npx npm-check --update --save-exact",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "net.namiking.interv-timer",
    "productName": "IntervTimer",
    "copyright": "Copyright Â© 2020 Tsubasa Namiki",
    "afterSign": "./bin/after-sign-mac.js",
    "directories": {
      "output": "release",
      "buildResources": "resources"
    },
    "files": [
      "dist"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    }
  },
  "dependencies": {
    "electron-devtools-installer": "3.2.0",
    "electron-log": "4.4.8",
    "electron-store": "8.1.0",
    "electron-updater": "5.3.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.2",
    "@mui/styles": "5.11.2",
    "@playwright/test": "1.29.1",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "4.0.0",
    "@types/jest": "29.2.5",
    "@types/papaparse": "5.3.7",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@vitejs/plugin-react": "3.0.0",
    "classnames": "2.3.2",
    "electron": "22.0.0",
    "electron-builder": "23.6.0",
    "electron-notarize": "1.2.2",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-standard": "5.0.0",
    "fast-deep-equal": "3.1.3",
    "i18next": "22.4.6",
    "jest": "29.3.1",
    "papaparse": "5.3.2",
    "playwright": "1.29.1",
    "prettier": "2.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "12.1.1",
    "react-redux": "7.2.2",
    "react-router-dom": "6.6.1",
    "react-test-renderer": "17.0.2",
    "redux": "4.2.0",
    "redux-observable": "2.0.0",
    "rxjs": "7.8.0",
    "ts-jest": "29.0.3",
    "typeface-roboto": "1.1.13",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vite-plugin-electron": "0.11.1",
    "vite-plugin-electron-renderer": "0.11.3"
  }
}
