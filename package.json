{
  "private": true,
  "name": "interv-timer",
  "description": "My application description",
  "version": "0.6.0-beta.5",
  "author": "Tsubasa Namiki <namikingsoft@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/namikingsoft/interv-timer.git"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "vite",
    "compile": "vite build",
    "build": "electron-builder",
    "build:dir": "npm run build -- --dir -c.compression=store -c.mac.identity=null",
    "release": "npm run build -- --publish always -c.win.verifyUpdateCodeSignature=false",
    "test": "jest",
    "e2e": "playwright test",
    "fmt": "prettier --write .",
    "fmtc": "prettier --check .",
    "type": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "upgrade-interactive": "npx npm-check --update --save-exact",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "net.namiking.interv-timer",
    "productName": "IntervTimer",
    "copyright": "Copyright Â© 2020 Tsubasa Namiki",
    "afterSign": "./bin/after-sign-mac.js",
    "directories": {
      "output": "release",
      "buildResources": "resources"
    },
    "files": [
      "dist"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    }
  },
  "dependencies": {
    "electron-devtools-installer": "3.2.0",
    "electron-log": "4.4.8",
    "electron-store": "8.1.0",
    "electron-updater": "5.3.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.12",
    "@playwright/test": "1.31.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.4.0",
    "@types/papaparse": "5.3.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitejs/plugin-react": "3.1.0",
    "electron": "23.1.3",
    "electron-builder": "23.6.0",
    "electron-notarize": "1.2.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-standard": "5.0.0",
    "fast-deep-equal": "3.1.3",
    "i18next": "22.4.11",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "papaparse": "5.4.0",
    "playwright": "1.31.2",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.9.0",
    "redux": "4.2.1",
    "redux-observable": "2.0.0",
    "rxjs": "7.8.0",
    "ts-jest": "29.0.5",
    "typeface-roboto": "1.1.13",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-plugin-electron": "0.11.1",
    "vite-plugin-electron-renderer": "0.12.1"
  }
}
